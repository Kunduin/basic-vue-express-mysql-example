{
  "root": true,
  "env": {
    "node": true,
    "browser": true
  },
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "vue/max-attributes-per-line": "off",
    "vue/require-prop-types":"off",
    "vue/require-default-prop":"off"
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
